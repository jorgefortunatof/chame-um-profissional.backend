{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"standard",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"camelcase": "off",
		"no-undef": "off",
		"prettier/prettier": "error",
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"no-tabs": 0
	}
}
